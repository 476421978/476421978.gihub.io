{
  "name": "blog",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "docs:dev": "yarn run register:components && vitepress dev docs",
    "docs:build": "yarn run register:components && vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "register:components": "vitepress-rc",
    "log": "conventional-changelog -p angular -i ./docs/关于/更新日志.md -s -r 0 -n ./changelog.ts",
    "prettier": "prettier --config ./.prettierrc.cjs --write \"./**/*.{js,vue}\" ",
    "commit": "git-cz"
  },
  "lint-staged": {
    "*.{js,vue,ts,jsx,tsx}": [
      "prettier --write"
    ],
    "*.{html,css,less,scss,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.1.0",
    "@fcli/vue-auto-scroll": "0.0.2",
    "@nutui/nutui-bingo": "^1.0.7",
    "@vue-office/pdf": "^2.0.10",
    "axios": "^1.5.0",
    "better-scroll": "^2.5.1",
    "busuanzi.pure.js": "^1.0.3",
    "cz-customizable": "^7.2.1",
    "dayjs": "^1.11.10",
    "echarts": "^5.4.3",
    "element-plus": "^2.3.12",
    "html2canvas": "^1.4.1",
    "js-export-excel": "^1.1.4",
    "jspdf": "^2.5.1",
    "mammoth": "^1.6.0",
    "mockjs": "^1.1.0",
    "mqtt": "^5.11.0",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.6",
    "print-js": "^1.6.0",
    "sass": "^1.66.1",
    "screenfull": "^6.0.2",
    "uuid": "^9.0.1",
    "vue": "^3.3.4",
    "vue-demi": "^0.14.6",
    "vue-request": "^2.0.4",
    "vue-router": "4",
    "vue-seamless-scroll": "^1.1.23",
    "vue3-seamless-scroll": "^2.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.9",
    "@babel/core": "^7.24.0",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@babel/preset-env": "^7.24.0",
    "@types/mockjs": "^1.0.10",
    "@types/node": "^20.5.9",
    "@vitejs/plugin-vue": "^4.2.3",
    "conventional-changelog-cli": "2.2.2",
    "cz-git": "^1.9.4",
    "less": "^4.2.0",
    "postcss-px-to-viewport-8-plugin": "^1.2.3",
    "typescript": "^5.1.6",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-vue-components": "^0.25.2",
    "vite": "^4.4.5",
    "vitepress": "^1.0.1",
    "vitepress-theme-demoblock": "^3.0.7",
    "vue-tsc": "^1.8.5",
    "xlsx": "^0.18.5"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git",
      "useEmoji": true,
      "czConfig": "./cz.config.cjs"
    }
  },
  "volta": {
    "node": "18.20.1"
  }
}
